 <%- include('../partial/header') -%>
     <%- include('sidebar') -%>

        <div class="main">
        <b><h1 style="font-size:xx-large;"></h1></b>
            <h2> Add Custom Attributes</h2>
            <form action="/api/admin/addcustom" method="post">
                        <div class="form-group">
                            <div class="field-wrap">
                                <label for="id-input" class="sr-only">ID</label>
                                <input type="text" name='id' class="form-control" placeholder="ID" id="name-input" required autocomplete="off" required autofocus />
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="field-wrap">
                                <label for="pass-input" class="sr-only"> Custom Attributes
                                </label>
                                <input type="text" name='attribute' class="form-control" placeholder="Custom Attribute" id="num-input" required autocomplete="off" required />
                            </div>
                        </div>
                        <button type=submit class="btn btn-primary button btn-block" >Submit</button>
            </form>
        </div>

        </body>
        </html>
