 <%- include('../partial/header') -%>
     <%- include('sidebar') -%>

        <div class="main container-fluid">
            <div class="alert text-lg-left">
                Add Additional Information
            </div>
        <hr>

            <% if (results.length > 0) {%>
            <form action="/api/sm/additionaldet" method="post">
                    
                <% results.forEach(function(result, index){ %>
                <p><%=result.custom_attr%></p>
                <input name="<%=result.id%>" ></input>
                <% }) %>
            
            <button type=submit class="btn btn-primary button btn-block" >Submit</button>
            </form>
                <% } else { %>
                    <p class="text-center">No custom attributes found.</p>
                <% } %>

            
        </div>
        </body>
        </html>
