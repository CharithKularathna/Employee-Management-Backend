<div class="table-wrapper main">
    <% if (PayGs.length > 0) {%>
    <table class="table table-hovered" border="1">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Pay Grade</th>
                <th scope="col">Employees</th>
                <th scope="col">Number of Employees</th>
            </tr>
        </thead>
        <tbody>
            <% PayGs.forEach(function(pay, index){ %>
            <tr>
                <td><%= pay.Pay_Grade %></td>
                <td><%= pay.Employees %></td>
                <td><%= pay.Count %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
    <% } else { %>
    <p class="text-center">No records found.</p>
    <% } %>
</div>
</body>

</html>